<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="collective.saml2">

    <browser:page
        name="bpm_redirect"
        for="*"
        permission="zope2.View"
        class=".BPM.BPM"
        />

    <browser:page
        name="app_login"
        for="*"
        permission="zope2.View"
        class=".App.Login"
        />

</configure>
